{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ES2021",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "./node_modules"
    ]
}